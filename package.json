{
  "name": "inlyne",
  "displayName": "Inlyne",
  "description": "VS Code sidebar editor with TipTap and live-sync via a Docker API",
  "version": "0.0.1",
  "publisher": "inlyneio",
  "type": "module",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onUri",
    "onCommand:inlyne.helloWorld",
    "onView:inlyne.sidebarView",
    "onCommand:inlyne.openEditorTab",
    "onStartupFinished",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:python",
    "onCommand:inlyne.helloWorld",
    "onView:inlyne.sidebarView",
    "onCommand:inlyne.openEditorTab"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "inlyne.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "inlyne.openEditorTab",
        "title": "Inlyne: Open Editor Tab"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "inlyne-sidebar",
          "title": "Inlyne",
          "icon": "media/extension.svg"
        }
      ]
    },
    "views": {
      "inlyne-sidebar": [
        {
          "id": "inlyne.sidebarView",
          "name": "Documentation",
          "type": "webview"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "inlyne.editor",
        "displayName": "Inlyne Editor Tab",
        "selector": [
          {
            "scheme": "inlyne-doc"
          },
          {
            "filenamePattern": "*.inlyne"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build:webview": "esbuild src/webview/editor.tsx --bundle --format=esm --outfile=media/editor.js --platform=browser --jsx=automatic --tsconfig=tsconfig.json --define:process.env.NODE_ENV=\\\"production\\\" --define:global=window --sourcemap --log-level=warning",
    "build:webview:webpack": "webpack --config webview.webpack.config.js"
  },
  "dependencies": {
    "@stomp/stompjs": "^7.1.1",
    "@tiptap/core": "^2.12.0",
    "@tiptap/extension-color": "^2.12.0",
    "@tiptap/extension-highlight": "^2.12.0",
    "@tiptap/extension-image": "^2.12.0",
    "@tiptap/extension-placeholder": "^2.12.0",
    "@tiptap/extension-text-align": "^2.12.0",
    "@tiptap/extension-text-style": "^2.12.0",
    "@tiptap/react": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "node-fetch": "^2.7.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sockjs-client": "^1.6.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/sockjs-client": "^1.5.4",
    "@types/stompjs": "^2.3.9",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.23.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}
